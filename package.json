{
  "name": "store-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "export ENV=test && tsc && db-migrate up --env test && tsc && jasmine && db-migrate reset",
    "test:win": "set ENV=test && tsc && db-migrate up --env test && tsc && jasmine && db-migrate reset",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "npm run build && node dist/index.js",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migration:run": "db-migrate up",
    "migration:drop": "db-migrate down",
    "migration:reset": "db-migrate reset"
  },
  "author": "Yousef Aboalata",
  "license": "ISC",
